{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["**/kubejs/**"]
		},
		"lint": {
			"dependsOn": [],
			"outputs": []
		},
		"bump": {
			"dependsOn": [],
			"outputs": ["CHANGELOG.md"]
		},
		"check-update": {
			"dependsOn": [],
			"outputs": []
		},
		"test": {
			"dependsOn": ["forgeworks#build"],
			"outputs": ["coverage/**"]
		},
		"predev": {
			"dependsOn": ["build"],
			"outputs": [
				"$TURBO_ROOT$/install/**",
				"$TURBO_ROOT$/environment/client/mods/**",
				"$TURBO_ROOT$/environment/client/kubejs/**",
				"$TURBO_ROOT$/environment/client/config/**",
				"$TURBO_ROOT$/environment/client/defaultconfigs/**",
				"$TURBO_ROOT$/environment/client/shaderpacks/**",
				"$TURBO_ROOT$/environment/server/mods/**",
				"$TURBO_ROOT$/environment/server/kubejs/**",
				"$TURBO_ROOT$/environment/server/config/**",
				"$TURBO_ROOT$/environment/server/defaultconfigs/**"
			]
		}
	}
}
